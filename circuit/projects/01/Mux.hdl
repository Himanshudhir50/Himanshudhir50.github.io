/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
   NOT(in=sel, out=notsel);
   AND(a=a, b=notsel, out=out);
   AND(a=b, b=sel, out=out2);
   OR()
}